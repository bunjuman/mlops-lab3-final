name: mlflow_lifecycle_pipeline

entry_points:
  ingest:
    command: "python src/ingest.py"

  train:
  parameters:
    data_path: "data/breast_cancer.csv"
    C: {type: float, default: 1.0}
    gamma: {type: string, default: "scale"}
  command: "python src/train.py --data_path {data_path} --C {C} --gamma {gamma}"

